<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerWithIndicatorView">
        <attr name="indicatorDrawable" format="reference"></attr>
        <attr name="indicatorMarginBottom" format="dimension"></attr>
        <attr name="indicatorMarginRight" format="dimension"></attr>
        <attr name="indicatorGravity" format="enum">
            <enum name="center_horizontal" value="0"></enum>
            <enum name="right" value="1"></enum>
            <enum name="left" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>